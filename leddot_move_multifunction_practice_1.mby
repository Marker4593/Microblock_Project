{"config.json":"{\"mode\":\"code\",\"github\":null,\"board\":\"openbit\",\"level\":\"Beginner\"}","main.py":"import display\nfrom machine import Pin\nfrom time import sleep\nfrom neopixel import NeoPixel\n\nnp = NeoPixel(Pin(13, Pin.OUT), 24)\nswleft = Pin(5, Pin.IN)\nsl_check = 1\n\nswright = Pin(12, Pin.IN)\nsr_check = 1\n\nswup = Pin(2, Pin.IN)\nsu_check = 1\n\nswdown = Pin(15, Pin.IN)\nsd_check = 1\n\nx = 0\ny = 0\n\ndef Dot_move(position, operation):\n    global x, y\n    if position == \"y\":\n        display.dot(x, y, (0, 0, 0))\n        if operation == \"down\":\n            y += 1\n            sleep(0.1)\n            if y > 4:\n                y -= 1\n                buzzer.tone(200, 1)\n\n        elif operation  == \"up\":\n            y -= 1\n            sleep(0.1)\n            if y < 0:\n                y += 1\n                buzzer.tone(200, 1)\n\n    elif position == \"x\":\n        display.dot(x, y, (0, 0, 0))\n        if operation == \"right\":\n            x += 1\n            sleep(0.1)\n            if x > 4:\n                x -= 1\n                buzzer.tone(200, 1)\n        elif operation == \"left\":\n            x -= 1\n            sleep(0.1)\n            if x < 0:\n                x += 1\n                buzzer.tone(200, 1)\n\n\ndef Runing():\n    global x, y, sd_check, su_check, sr_check, sl_check\n    display.dot(x, y, display.hex2byte('#ff0000'))\n    display.dot(2, 2, display.hex2byte('#33ff33'))\n\n    sd = swdown()\n    if sd == 0 and sd_check == 1:\n        Dot_move(\"y\", \"down\")\n    sd_check = sd\n\n    su = swup()\n    if su == 0 and su_check == 1:\n        Dot_move(\"y\", \"up\")\n    su_check = su\n\n    sr = swright()\n    if sr == 0 and sr_check == 1:\n        Dot_move(\"x\", \"right\")\n    sr_check = sr\n\n    sl = swleft()\n    if sl == 0 and sl_check == 1:\n        Dot_move(\"x\", \"left\")\n    sl_check = sl\n\n    if x == 2 and y == 2:\n        display.dot(x, y, display.hex2byte('#33ff33'))\n        buzzer.tone(200, 2)\n        display.scroll('Finish', color=display.hex2byte('#33ff33'))\n        x = 0\n        y = 0\n\n\n\n\nwhile True:\n    Runing()\n\n\n\n\n\n\n"}