Blockly.Python.valueToCode(block, 'data', Blockly.Python.ORDER_ATOMIC);\n  //var code = `Ultrasonic.read(32)`;\n  var code = `adc_ele.read(${value_seaLevelhPa},${value_seaLeveldata},${value_seaLeveltrim})`;\n  return [code, Blockly.Python.ORDER_NONE];\n};\n"},"extension.js":"({\n    name: \"analog electronics\", // Category Name\n    description: \"Get  distance from  analog-read\",\n    author: \"mazlogyzop\",\n    category: \"Sensors\",\n    version: \"1.0.1\",\n    icon: \"/static/sonar.png\", // Category icon\n    color: \"#004098\", // Category color (recommend some blocks color)\n    blocks: [ // Blocks in Category\n        {\n            xml: `\n                <block type=\"adc\">\n                    <value name=\"trig\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">32</field>\n                        </shadow>\n                    </value>\n                    <value name=\"data\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">10</field>\n                        </shadow>\n                    </value>\n                    <value name=\"trim\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">10</field>\n                        </shadow>\n                    </value>\n                </block>\n            `\n        }\n    ]\n});\n","modules":{"adc_ele.py":"from time import sleep\nfrom machine import Pin\nfrom machine import ADC\n\ndef read(analog_pin,A,B):\n    adc = ADC(Pin(analog_pin))\n    adc.atten(ADC.ATTN_11DB)\n    adc.width(ADC.WIDTH_12BIT)\n    #if A <= B :\n      #  B = 0\n    #if A <= B*2 :\n        #B = 0\n    T = A*(B / 100)/2\n    T = int (T)\n    if T == 0 :\n        T = 1\n    B = T\n    try:\n        a = []   \n        for count in range(A):\n            a.append(adc.read())\n            sleep(0.01)            \n        a.sort()\n        v = a[B:(A-B)]\n        avg = ( s