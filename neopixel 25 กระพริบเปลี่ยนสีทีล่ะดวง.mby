=auto\n","blocks":{"blocks.js":"Blockly.defineBlocksWithJsonArray(\n  [{\n    \"type\": \"airri_set_value\",\n    \"message0\": \"Set %1 to %2\",\n    \"args0\": [\n      {\n        \"type\": \"field_dropdown\",\n        \"name\": \"field_name\",\n        \"options\": [\n          [ \"Temperature (Â°C)\", \"temp\" ],\n          [ \"Humidity (%RH)\", \"humi\" ],\n          [ \"Pressure (hPa)\", \"pressure\" ],\n          [ \"Illuminance (lx)\", \"light\" ],\n          [ \"UV Index\", \"uv\" ],\n          [ \"PM1.0 (ug/m3)\", \"pm010\" ],\n          [ \"PM2.5 (ug/m3)\", \"pm025\" ],\n          [ \"PM10 (ug/m3)\", \"pm100\" ],\n          [ \"Wind Speed (m/s)\", \"wind_speed\" ],\n          [ \"Wind Direction (Â°)\", \"wind_dir\" ],\n          [ \"Rain Volume (cm)\", \"rain\" ],\n          [ \"CO2 (ppm)\", \"co2\"\n          ]\n        ]\n      },\n      {\n        \"type\": \"input_value\",\n        \"name\": \"value\",\n        \"check\": \"Number\"\n      }\n    ],\n    \"inputsInline\": true,\n    \"previousStatement\": null,\n    \"nextStatement\": null,\n    \"colour\": \"#2ecc71\",\n    \"tooltip\": \"\",\n    \"helpUrl\": \"\"\n  },\n  {\n    \"type\": \"airri_setup\",\n    \"message0\": \"Airri begin with  %1 WiFi Name: %2 WiFi Password: %3 Email: %4\",\n    \"args0\": [\n      {\n        \"type\": \"input_dummy\"\n      },\n      {\n        \"type\": \"input_value\",\n        \"name\": \"wifi_name\",\n        \"check\": \"String\",\n        \"align\": \"RIGHT\"\n      },\n      {\n        \"type\": \"input_value\",\n        \"name\": \"wifi_password\",\n        \"check\": \"String\",\n        \"align\": \"RIGHT\"\n      },\n      {\n        \"type\": \"input_value\",\n        \"name\": \"email\",\n        \"check\": \"String\",\n        \"align\": \"RIGHT\"\n      }\n    ],\n    \"previousStatement\": null,\n    \"nextStatement\": null,\n    \"colour\": \"#2ecc71\",\n    \"tooltip\": \"\",\n    \"helpUrl\": \"\"\n  },\n  {\n    \"type\": \"airri_set_station_name\",\n    \"message0\": \"Change station name to %1\",\n    \"args0\": [\n      {\n        \"type\": \"input_value\",\n        \"name\": \"name\",\n        \"check\": \"String\"\n      }\n    ],\n    \"inputsInline\": true,\n    \"previousStatement\": null,\n    \"nextStatement\": null,\n    \"colour\": \"#2ecc71\",\n    \"tooltip\": \"\",\n    \"helpUrl\": \"\"\n  },\n  {\n    \"type\": \"airri_set_station_location\",\n    \"message0\": \"Change station location to  %1 , %2\",\n    \"args0\": [\n      {\n        \"type\": \"input_value\",\n        \"name\": \"lat\",\n        \"check\": \"Number\"\n      },\n      {\n        \"type\": \"input_value\",\n        \"name\": \"lng\",\n        \"check\": \"Number\"\n      }\n    ],\n    \"inputsInline\": true,\n    \"previousStatement\": null,\n    \"nextStatement\": null,\n    \"colour\": \"#2ecc71\",\n    \"tooltip\": \"\",\n    \"helpUrl\": \"\"\n  },\n  {\n    \"type\": \"airri_push\",\n    \"message0\": \"Push data\",\n    \"previousStatement\": null,\n    \"nextStatement\": null,\n    \"colour\": \"#2ecc71\",\n    \"tooltip\": \"\",\n    \"helpUrl\": \"\"\n  }]\n);\n","generators.js":"Blockly.Python['airri_setup'] = function(block) {\n  Blockly.Python.definitions_['import_Airri'] = 'import Airri';\n\n  var value_wifi_name = Blockly.Python.valueToCode(block, 'wifi_name', Blockly.Python.ORDER_ATOMIC);\n  var value_wifi_password = Blockly.Python.valueToCode(block, 'wifi_password', Blockly.Python.ORDER_ATOMIC);\n  var value_email = Blockly.Python.valueToCode(block, 'email', Blockly.Python.ORDER_ATOMIC);\n\n  var code = `Airri.begin(${value_wifi_name}, ${value_wifi_password}, ${value_email})\\n`;\n  return code;\n};\n\nBlockly.Python['airri_set_station_name'] = function(block) {\n  Blockly.Python.definitions_['import_Airri'] = 'import Airri';\n\n  var value_name = Blockly.Python.valueToCode(block, 'name', Blockly.Python.ORDER_ATOMIC);\n\n  var code = `Airri.setStationName(${value_name})\\n`;\n  return code;\n};\n\nBlockly.Python['airri_set_station_location'] = function(block) {\n  Blockly.Python.definitions_['import_Airri'] = 'import Airri';\n\n  var value_lat = Blockly.Python.valueToCode(block, 'lat', Blockly.Python.ORDER_ATOMIC);\n  var value_lng = Blockly.Python.valueToCode(block, 'lng', Blockly.Python.ORDER_ATOMIC);\n\n  var code = `Airri.setLocation(${value_lat}, ${value_lng})\\n`;\n  return code;\n};\n\nBlockly.Python['airri_set_value'] = function(block) {\n  Blockly.Python.definitions_['import_Airri'] = 'import Airri';\n\n  var dropdown_field_name = block.getFieldValue('field_name');\n  var value_value = Blockly.Python.valueToCode(block, 'value', Blockly.Python.ORDER_ATOMIC);\n\n  var code = `Airri.setField(\"${dropdown_field_name}\", ${value_value})\\n`;\n  return code;\n};\n\nBlockly.Python['airri_push'] = function(block) {\n  Blockly.Python.definitions_['import_Airri'] = 'import Airri';\n\n  var code = 'Airri.push()\\n';\n  return code;\n};\n"},"extension.js":"({\n    name: \"Airri\", // Category Name\n    description: \"Send air quality value to Airri\",\n    author: \"microBlock\",\n    category: \"Communication\",\n    version: \"1.0.2\",\n    icon: \"/static/icon.png\", // Category icon\n    color: \"#2ecc71\", // Category color (recommend some blocks color)\n    blocks: [ // Blocks in Category\n        {\n            xml: `\n                <block type=\"airri_setup\">\n                    <value name=\"wifi_name\">\n                        <shadow type=\"text\">\n                            <field name=\"TEXT\">--WiFi-Name--</field>\n                        </shadow>\n                    </value>\n                    <value name=\"wifi_password\">\n                        <shadow type=\"text\">\n                            <field name=\"TEXT\">--WiFi-Password--</field>\n                        </shadow>\n                    </value>\n                    <value name=\"email\">\n                        <shadow type=\"text\">\n                            <field name=\"TEXT\">--Your-Email--</field>\n                        </shadow>\n                    </value>\n                </block>\n            `\n        },\n        {\n            xml: `\n                <block type=\"airri_set_station_name\">\n                    <value name=\"name\">\n                        <shadow type=\"text\">\n                            <field name=\"TEXT\">à¹à¸£à¸à¹à¸£à¸µà¸¢à¸à¹à¸¡à¹à¸à¸£à¸à¸¥à¹à¸­à¸</field>\n                        </shadow>\n                    </value>\n                </block>\n            `\n        },\n        {\n            xml: `\n                <block type=\"airri_set_station_location\">\n                    <value name=\"lat\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">0.0</field>\n                        </shadow>\n                    </value>\n                    <value name=\"lng\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">0.0</field>\n                        </shadow>\n                    </value>\n                </block>\n            `\n        },\n        {\n            xml: `\n                <block type=\"airri_set_value\">\n                    <value name=\"value\">\n                        <shadow type=\"math_number\">\n                            <field name=\"NUM\">0.0</field>\n                        </shadow>\n                    </value>\n                </block>\n            `\n        },\n        \"airri_push\",\n    ]\n});\n","modules":{"Airri.py":"from time import sleep\nimport network\nimport usocket\nimport ujson\nimport ussl\nimport ubinascii\nfrom machine import WDT\n\nHOST = \"airri.microblock.app\"\nPORT = const(443)\n\nWDT_TIMEOUT = 60 * 1000 # 60 s = 1 min\n\n_owner_email = \"\"\n_field = { }\n_ssid = \"\"\n_passw = None\n_wdt = None\n\nwlan = network.WLAN(network.STA_IF)\nmac_address = ubinascii.hexlify(wlan.config('mac'),':').decode().upper()\n\ndef connectWiFi():\n    print(\"WiFi Connecting\", end='')\n    wlan.active(True)\n    wlan.connect(_ssid, _passw)\n    if _wdt:\n        _wdt.feed()\n    while not wlan.isconnected():\n        sleep(0.1)\n        print(\".\", end='')\n    print(\"Connected\")\n    if _wdt:\n        _wdt.feed()\n\ndef begin(ssid, passw, owner_email):\n    global _ssid, _passw, _owner_email, _wdt\n    _ssid = ssid\n    _passw = passw if len(passw) > 4 else None\n    _owner_email = owner_email\n    _wdt = WDT(timeout=WDT_TIMEOUT)\n    connectWiFi()\n\ndef setStationName(name):\n    global _field\n    _field[\"name\"] = name.encode('utf8')\n\ndef setLocation(lat, lng):\n    global _field\n    _field[\"location\"] = \"{:.8f}, {:.8f}\".format(lat, lng)\n\ndef setField(name, value):\n    global _field\n    if not \"data\" in _field:\n        _field[\"data\"] = { }\n    _field[\"data\"][name] = float(value)\n\ndef push():\n    global _field\n    if _wdt:\n        _wdt.feed()\n    if not wlan.isconnected():\n        print(\"Error ! WiFi not connect\")\n        connectWiFi()\n    _field[\"email\"] = _owner_email\n    payload = str.encode(ujson.dumps(_field))\n    # print(\"Payload: \", payload)\n\n    s = usocket.socket()\n    ai = usocket.getaddrinfo(HOST, PORT)\n    try:\n        print(\"Connecting to \" + HOST + \"...\", end='')\n        s.connect(ai[0][-1])\n\n        if PORT == 443:\n            s = ussl.wrap_socket(s, server_hostname=HOST)\n        print(\" Connected\")\n        print(\"Send request:\")\n        dataReq =  (b\"POST /api/devices/{}/data HTTP/1.1\\r\\n\"\n                    b\"Host: {}:{}\\r\\n\"\n                    b\"Content-Type: application/json\\r\\n\"\n                    b\"Content-Length: {}\\r\\n\"\n                    b\"Connection: close\\r\\n\"\n                    b\"\\r\\n\").format(mac_address, HOST, PORT, len(payload))\n        dataReq = dataReq + payload\n        for line in str(dataReq, \"UTF-8\").split(\"\\r\\n\"):\n            print(\"> \" + str(line, \"UTF-8\"))\n        s.write(dataReq)\n\n        print(\"Response:\")\n        dataRes = s.read()\n        for line in str(dataRes, \"UTF-8\").split(\"\\r\"):\n            print(\"> \" + line.strip(\"\\n\"))\n        print(\"Disconnect\")\n        s.close()\n    except OSError:\n        print(\"Send data error !\")\n        s.close()\n\n    _field = { }\n    if _wdt:\n        _wdt.feed()\n    return None\n"},"static":{"icon.png":"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAIAAAACACAYAAADDPmHLAAAU+ElEQVR4nO2d\nCZQcxXnH/33MuTOzszN7r1a30IFAgFBkxBEQESAE+IhxBOYGQ8Avxg+/OIAj\nHBslMRiecRLbAgdzY0zACDsxEY7BCAkBkhCge3cl7aHVnrM799Ez051XNTPS\nzm73bM/OzF7dv/f2Iaa7q6r7+6rqq6++qmIkScI44gKwAsDpAE5L/80A4AZg\nA2Acz8JMIAKAIAAPgOMAmgAcBnAAwE4AA+NVtPFQgJUA/hrAFQCWkjxLneEU\nhwhkH4D/BfA6gI9K+TqlUoAGALcC+DqARaXIQEMcAvASgGcAdBb7tYutAPMA\nPADgRg015+MF6TZeAPCvAI4UK89iKUAdgI0AbgLAFyNBHUUSAJ4H8I8Augr9\nTIUqAAvgWwAeThtxOuMHMSI3APg3AOJEKMBcAC8COE8X+oSyHcANAFrHUgh2\njCW/BsAnuvAnBecD+BTAF8dSmLEowPcAbAZQPvm/jWYgsngjLZu8yKcLIMry\ncwB3af1rT3KeAnC3WrtArQJwAJ4GcPM0/nDTiecA3A4gOdo7qe0C/l0X/pTi\n5rTMRkWNAtyfblJ0phZ3p2WXk9G6gGvSBp/uv5+aEOF+FcBvlUqfSwFmp4cX\nurU/tfEBOBvAMbm3UOoCuPQEhC78qU952mHHyb2JkgIQ9+4qrX+5acQqpeG7\nXBfQkJ6C1H370wsvgIUAeoe+lVwLsFEX/rTECeCR4S82vAVYAmBvAXMEOpMb\nMpW8GEBLppTDBf2gLvxpDT/cNzC0BZiZjjTRAzqmNySyaD6ADgyr7bfpwtcE\nxvQ8ASXTAhBPXxuARq1/HY1Aav8s4inMtACrdOFrisaMnyejAGOKJtGZ0nxx\nqAJcoctSc1yOtA1Almv16zN+moMYf5WkBfiCLnxNQmS+kijAmVr/EhpmGZt2\nDepok8VsOvBDR5vMIQpQpQtfs9QRBbBr/StoGJeuANrGRvwA47pHjM7kQp/7\n1zi6Amic0s//S0kIJ36GeO/LSERbwfIV4J0Xwzz7YTCGyqJnJ8baEe/7L0gJ\nH3jHKvCuy6e1o1OUgPfbQvi0O4IGhxFXLrDBalBfr0tqA0jRZoQP3YpkaN/I\njI11sC74GTjn6qLll+j7DSIt34YkRk7+xrvXwXra0wBrLlo+k4VoQsJ33+7C\njo7wyRI1OAx4Ym09ZjsNqkpZui4gGUJo/7WywidIQhdCB65F0vtOUbKTYh0I\nt9ybJXxCwvM/ELp/VZQ8Jhu//GQwS/iETn8cj27vU13SkimA0PUkxOgou5ZI\nSUSOPUj/WyhJ/3ZAjJ5MhbMtg3n2D8DwLojBT0r1mhPKjvagbPb7uqOqi1Ui\nG0CE0P2sujvDh5AM7ADnuKCgHCUpu583Nt4Pg2stxFgnpIS3oLSnGmwe1bok\nLUDS/yE1xtQi9L5ScJ6c/ays10l6fk+7n6RvGzj78oLTn4wsrbHIlmpptXp7\npyQ