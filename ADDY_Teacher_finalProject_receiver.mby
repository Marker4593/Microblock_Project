ight(2, display.hex2byte('#00FFFF'))\nimport ESPNOW as now\nprint(now.getMyMAC())\n# 10:97:BD:21:2D:F0\n\n\nshow = True\nreceiv = True\n\nnum1, num2, num3, num4 = 0,0,0,0\naction = \"\"\nreturn_time = None\n\ndef led_on(color, *index):\n    for i in index:\n        np[i] = color\n    np.write()\n    sleep(0.15)\n\ndef led_off(*index):\n    for i in index:\n        np[i] = (0,0,0)\n    np.write()\n\ndef R_Top_scroll(color):\n    steps = [\n        ([0, 4], [4]),\n        ([5, 9, 3], [3, 9]),\n        ([10, 8, 14, 1, 2], [1, 2, 8, 14]),\n        ([15, 6, 7, 13, 19, 3], [3, 6, 7, 13, 19]),\n        ([8, 11, 12, 18, 24, 20, 1, 2], [20, 24, 15, 18, 10, 11, 12, 5, 8, 0, 1, 2]),\n        ([20, 23, 15, 16, 17, 10, 13, 5, 6, 7], [20, 23, 15, 16, 17, 10, 13, 5, 6, 7]),\n        ([20, 21, 22, 15, 18, 10, 11, 12], [20, 21, 22, 15, 18, 10, 11, 12]),\n        ([20, 23, 15, 16, 17], [20, 23, 15, 16, 17]),\n        ([20, 21, 22], [20, 21, 22]),\n    ] # Python  unpack tuple  \n\n    for on , off in steps:\n        led_on(color, *on) # *  unpack  list \n        led_off(*off)\n    sleep(0.5)\n# R_Top_scroll((30, 0, 30))\n\ndef rece_point():\n    global receiv, action\n    while receiv:\n        if now.isReadyToRead():\n            print(now.readAsText())\n            action = now.readAsText()\n            print(action)\n            manage_point(action)\n\n\n_thread.start_new_thread(rece_point, ())\n\ndef return_point(status, point):\n    global return_time\n    if point[0] == \"0\":\n        point = point[1:]\n        ready_send = point+status+return_time\n        now.send(ready_send, \"8C:CE:4E:BB:4A:FC\")\n    else:\n        ready_send = point+status+return_time\n        now.send(ready_send, \"8C:CE:4E:BB:4A:FC\")\n    sleep(0.01)\n\ndef show_num():\n    global num1, num2, num3, num4, show\n    while show:\n        iLED4.drawColon(True)\n        iLED4.writeDigitNum(0, num1, False)\n        iLED4.writeDigitNum(1, num2, False)\n        iLED4.writeDigitNum(2, num3, False)\n        iLED4.writeDigitNum(3, num4, False)\n        sleep(0.1)\n\n_thread.start_new_thread(show_num, ())\n\ndef manage_point(point):\n    global num1, num2, num3, num4, action\n    if action == \"A_UP\" or action == \"A_UPPER\":\n        if action == \"A_UP\":\n            num2 += 1\n        elif  action == \"A_UPPER\":\n            num2 += 2\n        if num2 > 9 and num1 != 9:\n            num2 = 0\n            num1 += 1\n        elif num2 > 9 and num1 == 9:\n            num2 = 0\n            num1 = 0\n        point = str(num1) + str(num2)\n        return_point(\"A_UP\", point)\n    elif action == \"A_DOWN\" or action == \"A_DOWNER\":\n        if action == \"A_DOWN\":\n            num2 -= 1\n        elif action == \"A_DOWNER\":\n            num2 -= 2\n        if num2 < 0 and num1 != 0:\n            num2 = 9\n            num1 -= 1\n        elif num2 < 0 and num1 == 0:\n            num2 = 9\n            num1 = 9\n        point = str(num1) + str(num2)\n        return_point(\"A_DOWN\", point)\n    \n    if action == \"B_UP\" or action == \"B_UPPER\":\n        if action == \"B_UP\":\n            num4 += 1\n        elif  action == \"B_UPPER\":\n            num4 += 2\n        if num4 > 9 and num3 != 9:\n            num4 = 0\n            num3 += 1\n        elif num4 > 9 and num3 == 9:\n            num4 = 0\n            num3 = 0\n        point = str(num3) + str(num4)\n        return_point(\"B_UP\", point)\n    elif action == \"B_DOWN\" or action == \"B_DOWNER\":\n        if action == \"B_DOWN\":\n            num4 -= 1\n        elif action == \"B_DOWNER\":\n            num4 -= 2\n        if num4 < 0 and num3 != 0:\n            num4 = 9\n            num3 -= 1\n        elif num4 < 0 and num3 == 0:\n            num4 = 9\n            num3 = 9\n        point = str(num3) + str(num4)\n        return_point(\"B_DOWN\", point)\n    print(\"Process Complete\")\n    \n\n\ndef main_process():\n    global return_time\n    display.show(\"R\", display.hex2byte('#FF00FF'))\n    now_time = time.localtime()\n    # print(now_time)\n    return_time = f\"{now_time[3]}:{now_time[4]}\"\n    # print(return_time)\n    sleep(0.01)\n\nwhile True:\n    main_process()\n"}